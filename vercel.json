{
  "builds": [
    {
      "src": "packages.txt",
      "use": "@vercel/build-env",
      "config": { "install": "apt-get" }
    },
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
